{
  "name": "create-node-express-app",
  "version": "0.1.2",
  "description": "create a new node application",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && eslint 'src/*.js' && babel ./src --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeputkarsh/create-node-express-app.git"
  },
  "keywords": [
    "node",
    "babel",
    "ES8",
    "express",
    "boilerplate"
  ],
  "author": "Utkarsh Deep",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deeputkarsh/create-node-express-app/issues"
  },
  "homepage": "https://github.com/deeputkarsh/create-node-express-app#readme",
  "preferGlobal": true,
  "bin": {
    "create-rest-api": "./dist/main.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "commander": "^5.1.0",
    "ejs": "^3.1.3",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "sorted-object": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "rimraf": "^3.0.2"
  }
}
