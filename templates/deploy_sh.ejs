<% if(!useEnv){ -%>
#!/bin/bash
<% } -%>
<% if(useMongoose){ -%>
  <%_ if(!useEnv){ %>export <% } %>MONGO_URI='<%- mongoUrl %>'
  <%_ if(!useEnv){ %>export <% } %>MONGO_DEBUG='<%- mongoDebug %>'
  <%_ if(!useEnv){ %>export <% } %>MONGO_SSL_CA=''
  <%_ if(!useEnv){ %>export <% } %>MONGO_SSL_CERT=''
  <%_ if(!useEnv){ %>export <% } %>MONGO_SSL_KEY=''
<% } -%>
<% if(useJwt){ -%>
  <%_ if(!useEnv){ %>export <% } %>JWT_SECRET=''
  <%_ if(!useEnv){ %>export <% } %>TOKEN_VALIDITY=''
<% } -%>
<% if(!useEnv){ %>export <% } %>NODE_ENV='<%- nodeEnv %>'
<% if(!useEnv){ %>export <% } %>PORT='<%- port %>'
<% if(useCors){ -%>
  <%_ if(!useEnv){ %>export <% } %>ALLOW_CORS_ORIGIN=''
  <%_ if(!useEnv){ %>export <% } %>ALLOW_CORS_METHODS=''
<% } -%>
<% if(!useEnv){ %>export <% } %>DEBUG='<%- debug %>'
<% if(!useEnv){ -%>
npm run dev
<% } -%>